{
  "name": "@saas-starter/db",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:migrate": "gel migrate",
    "db:seed": "gel query --file dbschema/seed.edgeql",
    "db:generate": "@gel/generate edgeql-js --target=ts",
    "db:reset": "gel database wipe --non-interactive && gel migrate && bun run db:seed",
    "postinstall": "bun run db:generate"
  },
  "dependencies": {
    "gel": "^2.1.0"
  },
  "devDependencies": {
    "@gel/generate": "^0.6.3",
    "typescript": "^5.4.5"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  }
}